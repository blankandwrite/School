<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="aqtc.gl.school">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".SchoolApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".main.home.activity.ScenceActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".main.home.activity.ScenceListActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".main.home.activity.ScenceDetailActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".main.home.activity.NewsListActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".main.home.activity.FaxListActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".main.home.activity.MediaListActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".main.home.activity.NoticeListActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".main.home.activity.ScienceListActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".main.find.ImagePagerActivity"/>
    </application>

</manifest>